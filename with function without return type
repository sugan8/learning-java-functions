public class Calculator {

  
    static void add(int num1, int num2) {
        int sum = num1 + num2;
        System.out.println("Sum: " + sum);

    }

    public static void main(String[] args) {
        int a = 10;
        int b = 20;

        // calling the function and storing the re
        add(a, b); 
    }
}
