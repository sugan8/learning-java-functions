// without arguments and with return

public class program {
    
    static int add() {
        int num1 = 10;
        int num2 = 20;
        int sum = num1 + num2; // like "returning" the sum
        return sum;
    }
    public static void main (String args[]){
        int result = add();
        System.out.println("The total value is: " + result);
    }
}
